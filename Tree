import java.util.LinkedList;
import java.util.List;
import java.util.Queue;
import java.util.Scanner;
import java.util.ArrayList;

public class Arbol {

    public static class TreeNode { //Genericos

        int valor;
        List<TreeNode> hijo = new ArrayList<>();

        TreeNode(int data) {
            valor = data;
        }

        TreeNode(int data, List<TreeNode> child) {
            valor = data;
            hijo = child;
        }
    }

    private static void imprimirArbol(TreeNode raiz) {
        if (raiz == null) {
            return;
        }
        Queue<TreeNode> queue = new LinkedList<>();
        queue.offer(raiz);
        while (!queue.isEmpty()) {
            int len = queue.size();
            for (int i = 0; i < len; i++) {
                TreeNode node = queue.poll();
                assert node != null; //Mientras nodo sea diferente de nulo le asigna valor al hijo
                //en caso de que no se cumpla nos mostrara un error y cortara el programa
                System.out.print(node.valor + " ");
                for (TreeNode item : node.hijo) {
                    queue.offer(item);
                    //en estas lineas asigna valor al hijo y verifica que el nodo al cual se
                    //le asginara como hijo, el padre no sea nulo.
                }
            }
            System.out.println();
        }
    }

    public static void main(String[] args) {
        
        //Aqui se crea y se le asigna valor al nodo raiz
        TreeNode raiz = new TreeNode(1);
        //Se le asignan hijos al nodo raiz con valores 2,3,4,5
        raiz.hijo.add(new TreeNode(2));
        raiz.hijo.add(new TreeNode(3));
        raiz.hijo.add(new TreeNode(4));
        raiz.hijo.add(new TreeNode(5));
//los nodos con valores 6, 7 y 8 son hijos del nodo con valor 2, el 9 es hijo del valor 3
//los nodos con valores 10,11 y 12 son hijos del nodo con valor 4
        raiz.hijo.get(0).hijo.add(new TreeNode(6));
        raiz.hijo.get(0).hijo.add(new TreeNode(7));
        raiz.hijo.get(0).hijo.add(new TreeNode(8));
        raiz.hijo.get(1).hijo.add(new TreeNode(9));
        raiz.hijo.get(2).hijo.add(new TreeNode(10));
        raiz.hijo.get(2).hijo.add(new TreeNode(11));
        raiz.hijo.get(2).hijo.add(new TreeNode(12));
//Este es el hijo del valor numero 6 a su vez se agrega un nuevo nivel
        raiz.hijo.get(2).hijo.get(0).hijo.add(new TreeNode(12)); 
//Este es el hijo del valor numero 7 que forma parte del nuevo nivel
        raiz.hijo.get(2).hijo.get(1).hijo.add(new TreeNode(13));
//Este es el hijo del valor numero 12 del 3 nivel (sin contar raiz)
        raiz.hijo.get(2).hijo.get(1).hijo.get(0).hijo.add(new TreeNode(13));
        imprimirArbol(raiz);
    }
}
